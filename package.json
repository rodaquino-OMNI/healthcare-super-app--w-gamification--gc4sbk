{
  "name": "austa-superapp",
  "version": "1.0.0",
  "private": true,
  "description": "AUSTA SuperApp - Journey-centered healthcare platform with gamification",
  "engines": {
    "node": ">=18.15.0",
    "yarn": ">=1.22.19"
  },
  "workspaces": [
    "src/backend/packages/*",
    "src/backend/api-gateway",
    "src/backend/auth-service",
    "src/backend/gamification-engine",
    "src/backend/health-service",
    "src/backend/care-service",
    "src/backend/plan-service",
    "src/backend/notification-service",
    "src/backend/shared",
    "src/web/design-system",
    "src/web/primitives",
    "src/web/interfaces",
    "src/web/journey-context",
    "src/web/shared",
    "src/web/mobile",
    "src/web/web"
  ],
  "dependencies": {
    "glob": "11.0.2",
    "npm-force-resolutions": "0.0.10"
  },
  "overrides": {
    "axios": "1.6.8",
    "semver": "7.5.4",
    "json5": "2.2.3",
    "minimatch": "3.1.2",
    "word-wrap": "1.2.4",
    "tough-cookie": "4.1.3",
    "postcss": "8.4.31",
    "@babel/traverse": "7.23.2",
    "follow-redirects": "1.15.4",
    "webpack": "5.76.0",
    "node-fetch": "2.6.9",
    "glob-parent": "5.1.2",
    "terser": "5.16.6",
    "loader-utils": "2.0.4",
    "minimist": "1.2.8",
    "shelljs": "0.8.5",
    "qs": "6.11.2",
    "ws": "8.16.0"
  },
  "resolutions": {
    "axios": "1.6.8",
    "semver": "7.5.4",
    "json5": "2.2.3",
    "minimatch": "3.1.2",
    "word-wrap": "1.2.4",
    "tough-cookie": "4.1.3",
    "postcss": "8.4.31",
    "@babel/traverse": "7.23.2",
    "follow-redirects": "1.15.4",
    "webpack": "5.76.0",
    "node-fetch": "2.6.9",
    "glob-parent": "5.1.2",
    "terser": "5.16.6",
    "loader-utils": "2.0.4",
    "minimist": "1.2.8",
    "shelljs": "0.8.5",
    "qs": "6.11.2",
    "ws": "8.16.0",
    "typescript": "5.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "@nestjs/core": "10.3.0",
    "@nestjs/common": "10.3.0",
    "@nestjs/graphql": "10.3.0",
    "@nestjs/platform-express": "10.3.0",
    "express": "4.18.2"
  },
  "scripts": {
    "postinstall": "node ./scripts/apply-security-patches.js && yarn workspaces run build",
    "fix-vulnerable-deps": "npm install --save-dev @babel/traverse@7.23.2 semver@7.5.4 json5@2.2.3 minimatch@3.1.2 word-wrap@1.2.4 tough-cookie@4.1.3 postcss@8.4.31 follow-redirects@1.15.4 webpack@5.76.0 node-fetch@2.6.9 glob-parent@5.1.2 terser@5.16.6 loader-utils@2.0.4 minimist@1.2.8 shelljs@0.8.5 qs@6.11.2 ws@8.16.0 --force && node ./scripts/apply-security-patches.js",
    "build": "yarn workspaces run build",
    "build:backend": "lerna run build --scope=@austa/backend-*",
    "build:web": "turbo run build --scope=@austa/web-*",
    "build:mobile": "cd src/web/mobile && yarn build",
    "build:packages": "yarn workspace @austa/design-system build && yarn workspace @design-system/primitives build && yarn workspace @austa/interfaces build && yarn workspace @austa/journey-context build",
    "test": "yarn workspaces run test",
    "test:backend": "lerna run test --scope=@austa/backend-*",
    "test:web": "turbo run test --scope=@austa/web-*",
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn workspaces run lint:fix",
    "validate-deps": "node ./scripts/validate-dependencies.js",
    "check-versions": "node ./scripts/check-versions.js",
    "clean": "yarn workspaces run clean",
    "dev:backend": "lerna run dev --parallel --scope=@austa/backend-*",
    "dev:web": "turbo run dev --parallel --scope=@austa/web-*",
    "dev:mobile": "cd src/web/mobile && yarn start"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "22.15.3",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@types/react-native": "0.72.8",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lerna": "8.1.2",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "turbo": "2.0.5",
    "typescript": "5.3.3"
  }
}
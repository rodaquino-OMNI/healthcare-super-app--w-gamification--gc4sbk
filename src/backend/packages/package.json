{
  "name": "@austa/backend-packages",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for AUSTA SuperApp backend packages",
  "workspaces": [
    "auth",
    "database",
    "errors",
    "events",
    "interfaces",
    "logging",
    "tracing",
    "utils"
  ],
  "scripts": {
    "build": "lerna run build --stream --concurrency=4",
    "build:watch": "lerna run build:watch --stream --parallel",
    "clean": "lerna run clean --stream --parallel",
    "lint": "lerna run lint --stream --parallel",
    "test": "lerna run test --stream",
    "test:watch": "lerna run test:watch --stream --parallel",
    "test:cov": "lerna run test:cov --stream",
    "publish:packages": "lerna publish --no-push",
    "version:packages": "lerna version --no-push --no-git-tag-version",
    "bootstrap": "lerna bootstrap --hoist",
    "postinstall": "yarn bootstrap"
  },
  "devDependencies": {
